#. Description
#: ../ubiquity.templates:28001
msgid "Your name:"
msgstr "Tên của bạn:"

#. Type: text
#. Description
#: ../ubiquity.templates:29001
msgid "Your name"
msgstr "Tên của bạn"

#. Type: text
#. Description
#: ../ubiquity.templates:30001
msgid "Pick a username:"
msgstr "Chọn tên đăng nhập:"

#. Type: text
#. Description
#: ../ubiquity.templates:31001
msgid "Username"
msgstr "Tên đăng nhập"

#. Type: text
#. Description
#: ../ubiquity.templates:32001
msgid "Choose a password:"
