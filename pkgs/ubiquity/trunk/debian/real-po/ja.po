#. Type: text
#. Description
#: ../ubiquity.templates:29001
msgid "Your name"
msgstr "あなたの名前"

#. Type: text
#. Description
#: ../ubiquity.templates:30001
msgid "Pick a username:"
msgstr "ユーザー名の入力:"

#. Type: text
#. Description
#: ../ubiquity.templates:31001
msgid "Username"
msgstr "ユーザー名"

#. Type: text
#. Description
#: ../ubiquity.templates:32001
msgid "Choose a password:"
msgstr "パスワードの入力:"

#. Type: text
#. Description
#: ../ubiquity.templates:33001
msgid "Password"
