#!/bin/sh
set -e

case "`archdetect`" in
	arm*/dove)
		exit 0
	;;
	arm*/imx51)
		exit 0
	;;
	arm*/iop32x)
		exit 0
	;;
	arm*/ixp4xx)
		exit 0
	;;
	arm*/kirkwood)
		exit 0
	;;
	arm*/orion5x)
		exit 0
	;;
	arm*/omap)
		exit 0
	;;
	arm*/omap4)
		exit 0
	;;
	# Don't activate it by default
	*)
		exit 1
	;;
esac

